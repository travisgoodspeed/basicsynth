	R, %4*23;

	'D = 14 e
	R, %4*3;
	G2, %4;
	R, %4;
	{G2, C3, G2}, {%8, %4, %8}, 100;

	'AAB x2
	loop 2 ' 11.5 * 2 = 23 q
	begin
		loop 2 { R, R, R, G2}, {%4, %4, %8, %8}, 100;
		{R, C3, C3, G2, R, C3, G2}, {%4, %4, %8, %4, %8, %4, %8}, 100;
	end

	'D
	set mnx2 = mnx2 + 0.5;
	param 0, mnx2;
	R, %4;
	{G2, C3, G2}, {%8, %4, %8}, 100;
	G2, %4;
	R, %4;
	{G2, C3, G2}, {%8, %4, %8}, 100;
	'F 
	G2, %2;
	G, %8;
	D3, %4;
	R, %2;
	D2, %4;
	R, %4*3;
	set mnx2 = mnx2 + 0.5;
	param 0, mnx2;

	' C
	loop 2
	begin
		param 0, mnx2;
		loop 2 ' AAB AAB 
		begin
			loop 2 { R, R, R, G2}, {%4, %4, %8, %8}, 100;
			{R, C3, C3, G2, R, C3, G2}, {%4, %4, %8, %4, %8, %4, %8}, 100;
		end

		'D = 14 e
		R, %4*3;
		G2, %4;
		R, %4;
		{G2, C3, G2}, {%8, %4, %8}, 100;

		set mnx2 = mnx2 + 0.5;
		param 0, mnx2;

		loop 2 'AAB AAB
		begin
			loop 2 {C2, G2, C3, G2}, {%4, %4, %8, %8}, 100;
			{G2, C3, C3, G2, G3, C3, G2}, {%4, %4, %8, %4, %8, %4, %8}, 100;
		end

		'D
		R, %4;
		{G2, C3, G2}, {%8, %4, %8}, 100;
		G2, %4;
		R, %4;
		{G2, C3, G2}, {%8, %4, %8}, 100;
		'F 
		G2, %2;
		G, %8;
		D3, %4;
		R, %2;
		D2, %4;
		R, %4*2;
		R, %8;
		D2, %8;

		set mnx2 = mnx2 + 0.5;
	end
'====================================================
	write "Plunk s1 = " :: curtime;
	'AAB AAB D
	R, %8*23;
	R, %8*23;
	R, %8*14;
	'AAB AAB D
	R, %8*23;
	R, %8*23;
	R, %8*14;
	' F F
	R, %8*19;
	R, %8*19;
	' AABDF
	R, %8*56;

	write "Plunk recap = " :: curtime;
	loop 2 'AABD AABD
	begin
		loop 2 {C2, G2, C3, G2}, {%4, %4, %8, %8}, 100;
		{G2, C3, C3, G2, G3, C3, G2}, {%4, %4, %8, %4, %8, %4, %8}, 100;

		'D
		R, %4;
		{G2, C3, G2}, {%8, %4, %8}, 100;
		G2, %4;
		R, %4;
		{G2, C3, G2}, {%8, %4, %8}, 100;
	end
	'F 
	G2, %2;
	G, %8;
	D3, %4;
	R, %2;
	D2, %4;
	R, %4*2;
	R, %8;
	D2, %8;

	write "Plunk coda = " :: curtime;
	R, %2;
	{C3, G2, C2}, {%4, %4, %4}, 100;
	write "Plunk end = " :: curtime;

end

