		{G3, G3, G, F}, {%16, %8, %16, %8};
		{G3, G3, G, F}, {%16, %8, %16, %8};
		R, %4;
		{G2, G, G, F}, {%16, %8, %16, %8};
		{G2, G, G, F}, {%16, %8, %16, %8};
		set mnx1 = mnx1 + 0.5;
	end
'=============================================
	param 0, 2.0;
	mixer send 0 ramp 0.8, 0.2, %8*120;
	mixer send 1 ramp 0.2, 0.0, %8*120;
write "Plonk1 s1 = " :: curtime;
	'AAB;
	{C3, E3, D#3, F, E, G, C, R}, {%8, %4, %8, %4, %8, %8, %4, %4}, 100;
	{Eb3, Eb3, Eb, D}, {%8, %4, %8, %4};
	{Eb3, Eb3, Eb, D}, {%8, %4, %8, %8};
	'AAB;

	{C3, E3, D#3, F, E, G, C, R}, {%8, %4, %8, %4, %8, %8, %4, %4}, 100;
	{G3, G3, G, F}, {%8, %4, %8, %4};
	{G3, G3, G, F}, {%8, %4, %8, %8};
	'D
	R, %4*7;

	param 0, 1.5;

	'AAB;
	{C3, E3, D#3, F, E, G, C, R}, {%8, %4, %8, %4, %8, %8, %4, %4}, 100;
	{Eb3, Eb3, Eb, D}, {%8, %4, %8, %4};
	{Eb3, Eb3, Eb, D}, {%8, %4, %8, %8};

	'AAB;
	{C3, E3, D#3, F, E, G, C, R}, {%8, %4, %8, %4, %8, %8, %4, %4}, 100;
	{G3, G3, G, F}, {%8, %4, %8, %4};
	{G3, G3, G, F}, {%8, %4, %8, %8};
	'D
	{Eb3, G3, R, F3, Bb3}, {%4, %2, %4, %2, %4}, 100;
	'F
	{G3, G3, G3, F3}, {%8, %4, %8, %8};
	{G3, G3, G3, F3}, {%8, %4, %8, %4};
	{Bb3, Bb3, Bb3}, {%4, %2, %4};

	'F
	{G3, G3, G3, G3}, {%4+%8, %2, %2, %1};
	
	' AABDF
	R, %8*56;
' RECAP ========================================================================
write "Plonk1 recap = " :: curtime;
	vol 90;
	param 0, 2.5;
	mixer send 0 set 0.8;
	mixer send 1 set 0.2;
	loop 2
	begin
		' AA = 23 e
		loop 2 {C3, E3, D#3, F, E, G, C, R}, {%16, %8, %16, %8, %16, %16, %8, %8}, 100;
		' B = 1 e
		{C3, E3, D#3, F, E, G, C},    {%16, %8, %16, %8, %16, %16, %8}, 100;
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};

		' D = 14 e
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
		R, %4;
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
		{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
	end
	'F = 19 e
	{C3, E3, D#3, F, E, G, C}, {%16, %8, %16, %8, %16, %16, %8}, 100;
	{G3, G3, G, F}, {%16, %8, %16, %8};
	{G3, G3, G, F}, {%16, %8, %16, %8};
	R, %4;
	{G2, G, G, F}, {%16, %8, %16, %8};
	{G2, G, G, F}, {%16, %8, %16, %8};

write "Plonk1 coda = " :: curtime;
	' 10 e
	R, %2;
	{C3, E3, D#3, F, E, G, C},    {%16, %8, %16, %8, %16, %16, %4}, 100;
write "Plonk1 end = " :: curtime;
