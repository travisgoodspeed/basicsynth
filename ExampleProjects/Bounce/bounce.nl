	mixer pan ramp 0.1, -0.8, 139;
'	mixer fxpan 0, ramp 0.0, 0.8, 139;

	set mnx1 = 0.5;
	loop 3  ' G = 139 e
	begin
		loop 2 ' E = 60 e
		begin
			param 0, mnx1;
			loop 2 ' C = 23 e
			begin
				' AA = 12 e
				loop 2 {C3, E3, D#3, F, E, G, C, R}, {%16, %8, %16, %8, %16, %16, %8, %8}, 100;
				' B = 11 e
				{C3, E3, D#3, F, E, G, C},    {%16, %8, %16, %8, %16, %16, %8}, 100;
				{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
				{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
			end
			' D = 14 e
			{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
			{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
			R, %4;
			{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
			{Eb3, Eb3, Eb, D}, {%16, %8, %16, %8};
